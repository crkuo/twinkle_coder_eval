# Twinkle Code Evaluator Environment Configuration
# Copy this file to .env and update the values

# Dataset cache directory (where benchmark datasets are stored)
DATASET_CACHE_FOLDER=cache

# OpenAI API Configuration (if using OpenAI backend)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com

# Custom API Configuration (if using custom OpenAI-compatible backend)
CUSTOM_API_KEY=your_custom_api_key_here
CUSTOM_BASE_URL=https://your-custom-api-url.com/api/

# Results directory (where evaluation results are stored)
RESULT_FOLDER=result

# Request handling configuration
# Maximum number of continuation rounds for OpenAI requests when finish_reason='length'
REQUESTS_ROUND_LIMIT=20

# Optional: Set to true to enable debug logging
DEBUG=false