# API Keys (REQUIRED - Replace with your actual keys)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com

# Dataset and Cache Settings
DATASET_CACHE_FOLDER=cache
RESULT_FOLDER=result

# Evaluation Default Settings
DEFAULT_NUM_SAMPLES=1
DEFAULT_NUM_WORKERS=1
DEFAULT_PASS_AT_K=1
DEFAULT_LOG_BATCH_SIZE=1

# Generation Parameters
DEFAULT_TEMPERATURE=0.0
DEFAULT_MAX_TOKENS=512
DEFAULT_TOP_P=1.0
DEFAULT_TIMEOUT=3.0

# OpenAI Specific Settings
REQUESTS_ROUND_LIMIT=20

# Processing Settings
OPEN_BATCH_LOG=false
MAX_WORKERS_AUTO_LIMIT=true
CONFIG_SIGNATURE_MAX_LENGTH=16

# Environment Settings
TOKENIZERS_PARALLELISM=false
DEBUG=false

# File Names (Optional - use if you want to customize output file names)
CONFIG_FILENAME=config.json
PROMPTS_FILENAME=prompts.jsonl
GENERATIONS_FILENAME=generations.jsonl
SOLUTIONS_FILENAME=solutions.jsonl
EVALUATIONS_FILENAME=evaluations.jsonl
RESULT_FILENAME=result.jsonl
GLOBAL_RESULTS_FILENAME=results.json

# Custom Backend Settings (if needed)
# CUSTOM_API_KEY=your_custom_api_key
# CUSTOM_BASE_URL=https://your-custom-api.com
# VLLM_API_KEY=your_vllm_api_key
# VLLM_BASE_URL=http://localhost:8000